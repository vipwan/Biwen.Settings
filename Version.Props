<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<PackageVersion>2.6.1</PackageVersion>
		<AutoClassGen>1.8.7</AutoClassGen>
		<PackageReleaseNotes>提供对IConfiguration和IOptions的集成支持</PackageReleaseNotes>
		<AssemblyVersion>$(PackageVersion)</AssemblyVersion>
		<FileVersion>$(PackageVersion)</FileVersion>
	</PropertyGroup>

	<PropertyGroup Label="Package versions used in this repository">
		<MicrosoftExtensionsVer>10.1.0</MicrosoftExtensionsVer>
		<STJ>10.0.1</STJ>
		<OrchardVer>2.2.1</OrchardVer>
	</PropertyGroup>

	<PropertyGroup Condition="'$(TargetFramework)' == 'net8.0'">
		<NetCoreMainVer>8.0.22</NetCoreMainVer>
	</PropertyGroup>
	<PropertyGroup Condition="'$(TargetFramework)' == 'net9.0'">
		<NetCoreMainVer>9.0.11</NetCoreMainVer>
	</PropertyGroup>
	<PropertyGroup Condition="'$(TargetFramework)' == 'net10.0'">
		<NetCoreMainVer>10.0.1</NetCoreMainVer>
	</PropertyGroup>

</Project>