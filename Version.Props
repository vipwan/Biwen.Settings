<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<PackageVersion>2.4.0</PackageVersion>
		<PackageReleaseNotes>提供对IConfiguration和IOptions的集成支持</PackageReleaseNotes>
		<AssemblyVersion>$(PackageVersion)</AssemblyVersion>
		<FileVersion>$(PackageVersion)</FileVersion>
	</PropertyGroup>

	<PropertyGroup Label="Package versions used in this repository">
		<FluentValidationAspNetCoreVer>11.3.0</FluentValidationAspNetCoreVer>
		<MicrosoftExtensionsVer>9.0.0</MicrosoftExtensionsVer>
		<OrchardVer>2.1.2</OrchardVer>
	</PropertyGroup>

	<PropertyGroup Condition="'$(TargetFramework)' == 'net8.0'">
		<NetCoreMainVer>8.0.11</NetCoreMainVer>
	</PropertyGroup>
	<PropertyGroup Condition="'$(TargetFramework)' == 'net9.0'">
		<NetCoreMainVer>9.0.0</NetCoreMainVer>
	</PropertyGroup>


</Project>